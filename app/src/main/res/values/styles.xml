<resources>


    <style name="Ttest" parent="@style/ThemeOverlay.AppCompat">
        <item name="android:colorForeground">@color/colorSecondaryForText</item>
        <item name="android:colorForegroundInverse">@color/colorSecondaryForText</item>
        <item name="colorControlNormal">?attr/colorPrimary</item>
        <item name="colorControlActivated">?attr/colorPrimary</item>
    </style>

    <style name="ThemeOverlay.My.White" parent="ThemeOverlay.AppCompat.ActionBar" >
        <item name="colorControlNormal">@color/colorReverce</item>
        <item name="android:textAppearanceLargePopupMenu">@style/TextAppearance.Colored</item>
        <item name="android:popupMenuStyle">@style/PopupTheme</item>
        <item name="android:textColorPrimary">@color/colorReverce</item>


    </style>

    <style name="ThemeOverlay.My.Colored" parent="ThemeOverlay.AppCompat.ActionBar" >
        <item name="android:popupMenuStyle">@style/PopupTheme</item>
        <item name="android:background">@color/colorReverce</item>
        <item name="colorControlNormal">@color/colorPrimary</item>
        <item name="android:textAppearanceLargePopupMenu">@style/TextAppearance.Colored</item>
        <item name="android:textColorPrimary">@color/colorPrimary</item>

    </style>

    <style name="PopupTheme" parent="ThemeOverlay.AppCompat.ActionBar">
        <item name="android:colorBackground">@color/colorReverce</item>
    </style>

    <style name="TextAppearance.Colored" parent="TextAppearance.AppCompat">
        <item name="android:textSize">14sp</item>
        <item name="android:textColor">@color/colorBlack</item>
    </style>


    <style name="TextAppearance.My" parent="TextAppearance.AppCompat">

    </style>

    <style name="TextAppearance.My.Body">
        <item name="android:textSize">14sp</item>
    </style>
    <style name="TextAppearance.My.Body.Primary">
        <item name="android:textColor">?android:colorPrimary</item>
    </style>

    <style name="Widget.Sample.Button.ButtonBar.AlertDialog" parent="Widget.AppCompat.Button.ButtonBar.AlertDialog">
        <item name="buttonBarPositiveButtonStyle">@color/colorSecondaryForTextInverce</item>
        <item name="buttonBarNegativeButtonStyle">@color/colorTrihderyForTextInverce</item>
        <item name="buttonBarNeutralButtonStyle">@color/colorPrimaryForTextInverce</item>
    </style>

    <style name="Widget.My.Button.Colored" parent="Widget.AppCompat.Button.Colored">
        <item name="background">@drawable/button_my</item>
    </style>

    <style name="Widget.AppCompat.Button.Borderless.Colored.OtherColor">
        <item name="android:textColor"> @color/colorSecondaryForTextInverce</item>
    </style>

    <style name="Widget.AppCompat.Button.Borderless.Colored.SecondColor">
        <item name="android:textColor"> @color/colorTrihderyForTextInverce</item>
    </style>


    <style name="TextAppearance.Sample" parent="TextAppearance.AppCompat">
    </style>

    <style name="TextAppearance.Sample.Body">

        <item name="android:textSize">16sp</item>
        <item name="android:textColor">?android:attr/textColorPrimary</item>


    </style>
    <style name="TextAppearance.Sample.Body.Primary">

        <item name="android:textColor">?android:attr/textColorSecondary</item>

    </style>

    <style name="TextAppearance.Sample.Body.Accent">

        <item name="android:textColor">?android:attr/textColorTertiary</item>

    </style>

    <style name="TextAppearance.Sample.Body.Headline">
        <item name="android:textColor">?android:attr/textColorPrimary</item>
        <item name="android:textSize">24sp</item>

    </style>

    <style name="TextAppearance.Sample.Body.Headline.Primary">

        <item name="android:textColor">?android:attr/textColorSecondary</item>

    </style>
    <style name="TextAppearance.Sample.Body.Headline.Accent">

        <item name="android:textColor">?android:attr/textColorTertiary</item>

    </style>

    <style name="Base.ThemeOverlay.AppCompat.ActionBar.my" parent="Base.ThemeOverlay.AppCompat.ActionBar">
        <item name="colorControlNormal">@color/colorReverce</item>
        <item name="android:textColor">@color/colorBlack</item>
        <item name="android:textColorPrimary">@color/colorReverce</item>

    </style>
    <style name="Base.TextAppearance.Widget.AppCompat.Toolbar.Title.My" parent="Base.TextAppearance.Widget.AppCompat.Toolbar.Title">
        <item name="android:textColor">@color/colorReverce</item>
    </style>

    <style name="Base.ThemeOverlay.AppCompat.ActionBar.other" parent="Base.ThemeOverlay.AppCompat.ActionBar">
        <item name="colorControlNormal">@color/colorReverce</item>
        <item name="android:textColor">@color/colorBlack</item>
        <item name="android:textColorPrimary">@color/colorReverce</item>

    </style>
</resources>
